<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <div class="panel panel-primary">
      <div class="panel-heading">
        Daily Reflections
      </div>
      <div class="panel-body">
        <%= render 'form', submit_message: "Update Note", user: @user, daily_note: @daily_note, readonly: false %>
      </div>
      <div class="panel-footer">
        <%= link_to 'Back', user_daily_notes_path(current_user),
                    class: "btn btn-default" %>
      </div>
    </div>
  </div>
</div>